{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "yarn"
  },
  "defaultProject": "plance",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "projects": {
    "plance": {
      "projectType": "application",
      "root": "apps/plance",
      "sourceRoot": "apps/plance/src",
      "prefix": "spitting-octo-fire",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/plance",
            "index": "apps/plance/src/index.html",
            "main": "apps/plance/src/main.ts",
            "polyfills": "apps/plance/src/polyfills.ts",
            "tsConfig": "apps/plance/tsconfig.app.json",
            "assets": ["apps/plance/src/favicon.ico", "apps/plance/src/assets"],
            "styles": ["apps/plance/src/styles.css"],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/plance/src/environments/environment.ts",
                  "with": "apps/plance/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "plance:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "plance:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "plance:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/plance/src/**/*.ts",
              "apps/plance/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/plance"],
          "options": {
            "jestConfig": "apps/plance/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "plance-e2e": {
      "root": "apps/plance-e2e",
      "sourceRoot": "apps/plance-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/plance-e2e/cypress.json",
            "tsConfig": "apps/plance-e2e/tsconfig.e2e.json",
            "devServerTarget": "plance:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "plance:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/plance-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "speech-temp": {
      "root": "libs/speech-temp",
      "sourceRoot": "libs/speech-temp/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/speech-temp/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/speech-temp"],
          "options": {
            "jestConfig": "libs/speech-temp/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
